{
  "compatibility_date": "2025-04-01",
  "main": "src/index.ts",
  "name": "aireclast",
  "upload_source_maps": true,
  "ai": {
    "binding": "AI"
  },
  "observability": {
    "enabled": true
  },
  "kv_namespaces": [
    {
      "binding": "AUTH_STORE",
      "id": "your-kv-namespace-id-here"
    }
  ],
  "vars": {
    "ALLOWED_EMAILS": "your-email@example.com",
    "JWT_SECRET": "your-secure-jwt-secret-here"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "src/static"
  }
}

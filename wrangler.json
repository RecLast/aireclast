{
  "compatibility_date": "2025-04-01",
  "main": "src/index.ts",
  "name": "aireclast",
  "upload_source_maps": true,
  "ai": {
    "binding": "AI"
  },
  "observability": {
    "enabled": true
  },
  "kv_namespaces": [
    {
      "binding": "AUTH_STORE",
      "id": "AuthIDishere"
    }
  ],
  "vars": {
    "ALLOWED_EMAILS": "yourmail@gmail.com",
    "JWT_SECRET": "your-secure-jwt-secret-here-replace-this-in-production"
  },
  "assets": {
    "binding": "ASSETS",
    "directory": "src/static"
  }
}
